<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.atden04.java.share_database_app.mvc.Controller">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem fx:id="addStockMenu" mnemonicParsing="false" onAction="#handleAddStock" text="Add Stock" />
                        <MenuItem fx:id="addDividendMenu" mnemonicParsing="false" onAction="#handleAddDividend" text="Add Dividend"/>
                        <MenuItem fx:id="addSaleMenu" mnemonicParsing="false" onAction="#handleAddSale" text="Add Sale"/>
                        <MenuItem fx:id="addLiquidationMenu" mnemonicParsing="false" onAction="#handleAddLiquidation" text="Add Liquidation"/>
                        <MenuItem fx:id="deleteRowMenu" mnemonicParsing="false" onAction="#handleDeleteRow" text="Delete Row" />
                        <MenuItem fx:id="outputData" mnemonicParsing="false" onAction="#handleOutputData" text="Output Data" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
            <children>
                <TabPane fx:id="tabPane" prefHeight="693.0" prefWidth="1019.0" tabClosingPolicy="UNAVAILABLE" />
            </children>
        </AnchorPane>
    </children>
</VBox>